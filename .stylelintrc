{
  "plugins": ["stylelint-scss"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extends", "tailwind", "mixin", "include"]
      }
    ]
  },
  "customSyntax": "postcss-scss",
  "extends": ["stylelint-config-recommended", "stylelint-config-prettier"],
  "syntax": "scss"
}
